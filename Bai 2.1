#include <iostream>
using namespace std;
int timso(int n){
    int so=n;
    int count =0;
    while (n!=0){
        int chucai = n%10;
        if((chucai!=0)&&(so%chucai==0)){
            count ++;
    }
    n/=10;
}
return count;}



int main(){
    int a;
    cin >> a;
    while(a--){
    
        int n;
        cin >> n;
        cout << timso(n) << endl;}
        
        return 0;

        
    
    
}
