#include <iostream>
#include <string>
using namespace std;

int lacda(string s) {
    int count = 1; 
    for (char c : s) {// dat 1 bien c chay qua tung ki tu s
        if (isupper(c)) { // ham kiem tra xem ki tu co chu hoa k
            count++; 
        }
    }
    return count;
}

int main() {
    string s;
    
    cin >> s;
    cout  << lacda(s) << endl;
    return 0;
}
