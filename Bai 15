#include <iostream>
#include <string>
using namespace std;

int lacda(string s) {
    int count = 1; 
    for (char c : s) {
        if (isupper(c)) {
            count++; 
        }
    }
    return count;
}

int main() {
    string s;
    
    cin >> s;
    cout  << lacda(s) << endl;
    return 0;
}
