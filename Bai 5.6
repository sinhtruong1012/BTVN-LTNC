#include <map>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;
int main() {
   map<string, int>m;
   int x; cin >> x;
   for(int i = 0; i < x; i++){
       int a;
       cin >> a;
       int mark;
       string name;
        if(a == 1){
            cin >> name >> mark;
            m[name] += mark;
        } else if (a == 2) {
            cin >> name;
            m.erase(name);
        } else if ( a == 3){
            cin >> name;
            cout << m[name]<<endl;
        }
   }
     
    return 0;
}
