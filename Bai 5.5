#include <set>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;
int main() {
    int x;
    cin >> x;
    set<int>num;
    for(int i = 0; i < x; i++){
        int a,b; cin >> a >> b;
        if(a == 1){
            num.insert(b);
        } else if ( a == 2) {
           num.erase(b);
        } else if (a == 3){
            set<int>::iterator itr = num.find(b);
            if(itr!=num.end()){ cout << "Yes" << endl;}
            else { cout << "No" << endl;}
        }
    }
    return 0;
}
