
#include <bits/stdc++.h>
using namespace std;
 
int posW = -1, posH = -1;
int w , h; // w : row h : col
bool check = false;
bool pass[1001][1001] = {0};

void solve(int x , int y , char arr[][1001])
{
    if(x == h - 1) {check = true ; return;}
    
    if(x + 1 <= h && arr[x + 1][y] == 'E' && !pass[x + 1][y])
    {
        pass[x][y] = 1;
        solve(x + 1 , y , arr);
    }
    
    if(y + 1 <= w && arr[x + 1][y + 1] == 'E'&& arr[x][y + 1] == 'E' && !pass[x + 1][y + 1])
    {
        pass[x][y] = 1;
        solve(x + 1, y + 1, arr);
    }
    
    if(y - 1 >= 0 && arr[x + 1][y - 1] == 'E' && arr[x][y - 1] == 'E'&& !pass[x][y - 1]) 
    {
        pass[x][y] = 1;
        solve(x + 1, y - 1, arr);
    }
    
}

int main()
{
    cin >> w >> h;
    char a[1001][1001];
    for(int i = 0 ; i < h ; ++i)
        for(int j = 0 ; j < w ; ++j)
        {
            cin >> a[i][j];
            if(a[i][j] == 'Y')
            {
                posH = i ;posW = j;
            }
        }
    
    solve(posH , posW, a);
    if(check) cout << "YES" <<endl;
    else cout << "NO" <<endl;
    return 0;
}
